.faq-list
  margin -30px 0 0

  @media (min-width $screen-sm-min)
    margin-top 0

  @media (min-width $screen-md-min)
    .container-fluid
      position relative
      &:before
        content ''
        position absolute
        top 0
        left 365px
        bottom 0
        border-right 1px solid #ececec
        display block