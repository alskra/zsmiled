mixin social-networks()
  .-social-networks&attributes(attributes)
    +a().__item.glyphicon.glyphicon-gp(target='_blank' title='Google+')
    +a().__item.glyphicon.glyphicon-fb(target='_blank' title='Facebook')
    +a().__item.glyphicon.glyphicon-tw(target='_blank' title='Twitter')
    +a().__item.glyphicon.glyphicon-ins(target='_blank' title='Instagram')